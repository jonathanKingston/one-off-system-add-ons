[
  {
    "namespace": "experiments.rollout",
    "description": "Rollout",
    "types": [
      {
        "id": "pref",
        "type": "object",
        "description": "Preference to set",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "value": {
            "type": "any"
          }
        }
      },
      {
        "id": "button",
        "type": "object",
        "description": "Button object",
        "properties": {
          "label": {
            "type": "string"
          },
          "accessKey": {
            "type": "string"
          },
          "callbackId": {
            "type": "string"
          }
        }
      }
    ],
    "functions": [
      {
        "name": "showPrompt",
        "type": "function",
        "description": "Calls the showPrompt() function",
        "parameters": [
          {
            "type": "string",
            "name": "message",
            "description": "A message to show in the prompt"
          },
          {
            "type": "array",
            "name": "buttons",
            "description": "An array of prefNames",
            "items": { "$ref": "button" }
          }
        ],
        "async": true
      },
      {
        "name": "setPrefs",
        "type": "function",
        "description": "Calls the setPrefs() function",
        "parameters": [
          {
            "type": "array",
            "name": "prefs",
            "description": "An array of prefs",
            "items": { "$ref": "pref" }
          }
        ],
        "async": true
      },
      {
        "name": "getPrefs",
        "type": "function",
        "description": "Calls the getPrefs() function",
        "parameters": [
          {
            "type": "array",
            "name": "prefNames",
            "description": "An array of prefNames",
            "items": { "type": "string" }
          }
        ],
        "async": true
      }
    ],
    "events": [
      {
        "name": "promptResponse",
        "type": "function",
        "description": "Fired when a prompt responds.",
        "parameters": [
          {
            "type": "object",
            "name": "changeInfo",
            "properties": {
              "name": {
                "type": "string",
                "description": "Response name"
              }
            }
          }
        ]
      }
    ]
  }
]
